property ancestor, pSwitchType, pRetractingglobal gActorson new me, pX, pY, pZ, spriteMember, tileType, walk, switchSet  me.ancestor = script("tile3D").new(pX, pY, pZ, spriteMember, tileType, walk)  pSwitchType = switchSet  gActors.append(me)  return meendon retract me  pRetracting = 1endon getSwitchGroup me  return pSwitchTypeendon step me  if pRetracting = 1 then    me.pSprite.translate(0, -1, 0)    if me.pSprite.transform.position.y < -55 then      me.pWalkeable = 1      me.stopStepFrame()    end if  end ifend