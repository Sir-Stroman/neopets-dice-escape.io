property pLocX, pLocZ, pSprite, pType, pWalkeable, deleteMeon new me, pX, pY, pZ, spriteMember, tileType, walk  pSprite = spriteMember  pType = tileType  pWalkeable = walk  deleteMe = 0  me.setLoc(pX, pY, pZ)  return meendon dieOnGridLoc me, playerendon getWalkeable me  return pWalkeableendon getType me  return pTypeendon setLoc me, pX, pY, pZ  pSprite.transform.position = vector(pX, pY, pZ)endon stopStepFrame me  deleteMe = 1end