property pGame, pimage, porigimage, pcamera, pfontcoloron new me, i, my_camera, camera_texture, gameObj  pimage = i  porigimage = i.duplicate()  pfontcolor = rgb(255, 255, 255)  pcamera = my_camera  pGame = gameObj  pcamera.addOverlay(camera_texture, point(0, -20), 0)  return meendon resetTexture me  pGame.pWorld.texture("cameraTex").image = pimageendon drawPrompt me, prompt_num  pimage = porigimage.duplicate()  header = member("header_txt", "art")  bodyText = member("body_txt", "art")  header.color = rgb(255, 0, 0)  bodyText.color = rgb(255, 0, 0)  case prompt_num of    1:      header.text = "LEVEL" && pGame.getLevel() && "CLEAR!!!"      bodyText.text = "ROUND SCORE:" && pGame.getRoundScore() & RETURN & "TIME REMAINING:" && pGame.getNumSecondsLeft() & RETURN & "TIME BONUS:" && pGame.getTimeBonus() & RETURN & "YOUR SCORE:" && pGame.getScore() & RETURN & "<press any key to continue>"      make_buttons = 0    2:      header.text = "GAME OVER"      bodyText.text = "YOU HAVE NO LIVES LEFT" & RETURN & "YOUR SCORE:" && pGame.getScore()      make_buttons = 1    3:      header.text = "YOU CLEARED ALL LEVELS!!!"      bodyText.text = "ROUND SCORE:" && pGame.getRoundScore() & RETURN & "TIME REMAINING:" && pGame.getNumSecondsLeft() & RETURN & "TIME BONUS:" && pGame.getTimeBonus() & RETURN & "YOUR SCORE:" && pGame.getScore()      make_buttons = 1    4:      header.text = "GAME OVER"      bodyText.text = "YOUR SCORE:" && pGame.getScore()      make_buttons = 1  end case  image_alpha_clr = rgb(20, 20, 20)  new_image = image(480, 360, 32)  new_image.fill(0, 0, 480, 360, image_alpha_clr)  if make_buttons = 1 then    button_text = member("button_txt", "art")    button_text.text = "Send Score"    button_img = member("sbg", "art").image.duplicate()    endgamebutton_img = getbuttonwithtext(button_text, pfontcolor, button_img, image_alpha_clr)    xStart = 470 - endgamebutton_img.width    yStart = 350 - endgamebutton_img.height    destRect = rect(xStart, yStart, xStart + endgamebutton_img.width, yStart + endgamebutton_img.height)    new_image.copyPixels(endgamebutton_img, destRect, endgamebutton_img.rect)    button_text = member("button_txt", "art")    button_text.text = "Restart Game"    button_img = member("sbg", "art").image.duplicate()    endgamebutton_img = getbuttonwithtext(button_text, pfontcolor, button_img, image_alpha_clr)    xStart = 10    yStart = 350 - endgamebutton_img.height    destRect = rect(xStart, yStart, xStart + endgamebutton_img.width, yStart + endgamebutton_img.height)    new_image.copyPixels(endgamebutton_img, destRect, endgamebutton_img.rect)  end if  header.color = rgb(0, 0, 0)  bodyText.color = rgb(0, 0, 0)  header_img = header.image  body_img = bodyText.image  xStart = 242 - (header_img.width / 2)  yStart = 102 - (header_img.height / 2)  destRect = rect(xStart, yStart, xStart + header_img.width, yStart + header_img.height)  new_image.copyPixels(header_img, destRect, header_img.rect)  xStart = 242 - (body_img.width / 2)  yStart = 152  destRect = rect(xStart, yStart, xStart + body_img.width, yStart + body_img.height)  new_image.copyPixels(body_img, destRect, body_img.rect)  header.color = rgb(255, 255, 255)  bodyText.color = rgb(255, 255, 255)  header_img = header.image  body_img = bodyText.image  xStart = 240 - (header_img.width / 2)  yStart = 100 - (header_img.height / 2)  destRect = rect(xStart, yStart, xStart + header_img.width, yStart + header_img.height)  new_image.copyPixels(header_img, destRect, header_img.rect)  xStart = 240 - (body_img.width / 2)  yStart = 150  destRect = rect(xStart, yStart, xStart + body_img.width, yStart + body_img.height)  new_image.copyPixels(body_img, destRect, body_img.rect)  pimage.copyPixels(new_image, pimage.rect, pimage.rect, [#bgColor: image_alpha_clr, #ink: 36])  me.resetTexture()end