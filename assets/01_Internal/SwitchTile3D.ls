property ancestor, faceValue, pGame, pSwitchType, pToggledon new me, pX, pY, pZ, spriteMember, tileType, walk, switchSet, faceVal, gameObj  me.ancestor = script("tile3D").new(pX, pY, pZ, spriteMember, tileType, walk)  pToggled = 0  pSwitchType = switchSet  pGame = gameObj  faceValue = faceVal  return meendon dieOnGridLoc me, player  playerFace = player.getFace()  if (playerFace = faceValue) and (pToggled = 0) then    pToggled = 1    sound(1).play(member("triggerSound", "sounds"))    pGame.playerOnSwitch(pSwitchType, me.pSprite)  end ifend