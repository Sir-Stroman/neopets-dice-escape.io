global _DGSBIOS, gCursorState, gActorson prepareMovie  member("debug_txt").text = EMPTY  startTimer()  puppetTempo(999)  gActors = []  member("end_of_game_txt", "art").text = EMPTYendon startMovie  _DGSBIOS = script("dgsbios_class_bios").new()  _DGSBIOS.init(1, 10, 40, 300, 420, 125, "introframe", "introframe")  cursor(-1)  gCursorState = -1  go("_BIOS")endon stopMovie  doCleanup()  _DGSBIOS.destroy()end