property pGame, ancestor, faceValueon new me, pX, pY, pZ, spriteMember, tileType, walk, faceVal, gameObj  me.ancestor = script("tile3D").new(pX, pY, pZ, spriteMember, tileType, walk)  pGame = gameObj  faceValue = faceVal  return meendon dieOnGridLoc me, player  playerFace = player.getFace()  if playerFace = faceValue then    updateStage()    pGame.reachedGoal()    pGame.removetile(me.pSprite)  end ifend