global gActors, gmenuon exitFrame me  rendermode = getRendererServices()  rendermode.textureRenderFormat = #rgba4444  menutexture = member("plainTile", "3d Cast")  menuframetexture = member("menu_options_texture", "3d Cast")  instructionsframetexture = member("menu_instructions_texture", "3d Cast")  gmenu = script("MenuClass").new(menutexture.image, menuframetexture.image, instructionsframetexture.image)  gActors.append(gmenu)end