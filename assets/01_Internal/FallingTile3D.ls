property ancestor, psteppedon, pRetracting, pGame, pgyglobal gActorson new me, pX, pY, pZ, spriteMember, tileType, walk, gameObj  me.ancestor = script("tile3D").new(pX, pY, pZ, spriteMember, tileType)  pGame = gameObj  psteppedon = 0  pgy = 5  me.pWalkeable = 1  gActors.append(me)  return meendon dieOnGridLoc me, player  psteppedon = 1endon fall me  me.pWalkeable = 0  me.pSprite.translate(0, -pgy, 0)  pgy = pgy + 2  if me.pSprite.transform.position.y < -2500 then    me.stopStepFrame()    pGame.removetile(me.pSprite)  end ifendon step me  if psteppedon = 1 then    if pGame.getplayercurrenttile() <> me.pSprite then      me.fall()    end if  end ifend