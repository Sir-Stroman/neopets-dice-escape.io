global gActors, gLastStepFrame, gStartTimeon callStepFrame  now = the milliSeconds - gStartTime  if now < 12 then    return   end if  gStartTime = the milliSeconds  startTimer()  deleteList = []  repeat with x in gActors    if x.deleteMe = 1 then      deleteList.append(x)      next repeat    end if    x.step()  end repeat  repeat with x = 1 to deleteList.count    obj = deleteList[x]    gActors.deleteOne(obj)  end repeatend